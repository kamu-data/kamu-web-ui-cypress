{
  "name": "kamu-platform-e2e-cypress",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write '**/*.{ts,js,html,spec.ts}'",
    "prepare": "husky install",
    "install_pretty_quick": "npx husky set .husky/pre-commit \"npx pretty-quick --staged\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "rxjs": "~6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "cypress": "^10.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "~8.3.0",
    "typescript": "4.6.4"
  }
}
